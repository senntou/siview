/* ファイルリストパネル */
#fileListPanel {
    background-color: #1e1e1e;
    color: #dddddd;
    border: none;
}

#fileListPanel::item {
    padding: 6px;
}

#fileListPanel::item:selected {
    background-color: #007acc;
    color: white;
}

/* 画像ビューア */
#imageViewer {
    background-color: #1e1e1e;
}

#imageLabel {
    background-color: #1e1e1e;
}

#textView {
    background-color: #252526;
    color: #dddddd;
    border: none;
}
